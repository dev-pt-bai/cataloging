{
    "app": {
        "port": 8080,
        "tokenExpiry": 0
    },
    "secret": {
        "jwt": "yourJWTSecret"
    },
    "database": {
        "user": "youruser",
        "password": "yourpassword",
        "name": "yourdbname"
    },
    "external": {
        "msGraph": {
            "tenantID": "yourMSGraphAPITenantID",
            "clientID": "yourMSGraphAPIClientID",
            "redirectURI": "yourMSGraphAPIRedirectURI"
            "scopes": "https://graph.microsoft.com/.default",
            "directoryName": "yourOnedriveDirectoryName",
            "driveID": "yourOnedriveDriveID",
            "maxFileSize": 0,
            "supportedFileExt": [
                ".pdf",
                ".jpg",
                ".jpeg",
                ".png"
            ],
            "refreshInterval": 0
        }
    }
}